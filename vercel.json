{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd oja && npm run build",
  "outputDirectory": "oja/.next",
  "framework": "nextjs",
  "installCommand": "npm install && cd oja && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.ts"
    }
  ]
}
